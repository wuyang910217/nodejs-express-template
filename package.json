{
  "name": "express-template",
  "version": "1.0.3",
  "description": "nodejs express开发模板",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "dev": "cross-env DEBUG=maodou* NODE_ENV=development webpack --config webpack.config.js --progress --watch",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "start": "node dist/index.js",
    "release": "np --no-cleanup --yolo --no-publish",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [],
  "author": "wuyang",
  "homepage": "https://github.com/wuyang910217/node-express-template",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^2.6.9",
    "cors": "^2.8.5",
    "debug": "^3.2.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "maodou-logger-util": "^1.0.3",
    "maodou-redis-util": "^1.0.2",
    "mongoose": "^5.6.9",
    "qs": "^6.8.0"
  },
  "peerDependencies": {
    "debug": "^3.2.6",
    "core-js": "^2.6.9",
    "@babel/runtime": "^7.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime-corejs2": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "nodemon-webpack-plugin": "^4.0.8",
    "np": "^5.0.3",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "eslintIgnoreConfig": [
    "node_modules/",
    "coverage/",
    "dist/"
  ]
}
