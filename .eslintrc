{
  "env": {
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "extends": ["plugin:prettier/recommended", "plugin:node/recommended-module"],
  "rules": {
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-cond-assign": "error",
    "no-param-reassign": "warn",
    "no-console": "warn",
    "node/no-unpublished-require": "warn"
  }
}
